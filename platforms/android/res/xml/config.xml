<?xml version='1.0' encoding='utf-8'?>
<widget android-versionCode="8" defaultlocale="zh-TW" id="bc.ptcontent" version="1.8.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:vs="http://schemas.microsoft.com/appx/2014/htmlapps">
    <preference name="loglevel" value="DEBUG" />
