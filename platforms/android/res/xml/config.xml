    <plugin name="cordova-plugin-battery-status" src="https://github.com/apache/cordova-plugin-battery-status.git" version="1.1.2-dev" />
    <plugin name="cordova-plugin-networkinterface" src="https://github.com/salbahra/cordova-plugin-networkinterface.git" version="1.0.8" />
